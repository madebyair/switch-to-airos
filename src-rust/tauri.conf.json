{
  "productName": "switch-to-airos",
  "version": "1.0.0",
  "identifier": "com.air.switch",
  "build": {
    "beforeDevCommand": "npm run dev-vite",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build-vite",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Switch to airos",
        "width": 1000,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
